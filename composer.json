{
    "name": "shetabit/components",
    "description": ":description",
    "license": "license",
    "authors": [
        {
            "name": "Ali Nazari",
            "homepage": "https://github.com/AliN11"
        }
    ],
    "homepage": "https://github.com/shetabit/components",
    "keywords": ["Laravel", "Components"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shetabit\\Components\\": "src/"
        }
        "files": [
            "src/Helpers/functionss.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shetabit\\Components\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shetabit\\Components\\ComponentsServiceProvider"
            ],
            "aliases": {
                "Components": "Shetabit\\Components\\Facades\\Components"
            }
        }
    }
}
